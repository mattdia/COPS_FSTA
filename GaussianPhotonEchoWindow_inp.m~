%Gaussian window function for photon echo. Assumes you've run the master
%FTSA file

stdev_window = 5;
norm = 1/(sqrt(2*pi)*stdev_window);
pix_slope = (19/20);
pix_offs = -4;
windowed = [];
for j = 1:Numstem
    offs(j) = j-4;
    for i =1:40
        dummy_filt(i,j) = Z1plot(i,j)*exp(-(i-offs(j))^2/(2*stdev_window^2));
    end
end